<AnchorPane id="AnchorPane" fx:id="paneAdmin" prefHeight="614.0" prefWidth="908.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.GUI.AdminController">
